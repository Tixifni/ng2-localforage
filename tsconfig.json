{
  "compilerOptions": {
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "ES5",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "declaration": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "typeRoots": [ "node_modules/@types" ]
  },
  "angularCompilerOptions": {
    "genDir": "compiled"
  },
  "files": [
    "src/index.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
